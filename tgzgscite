cd ..
rm -f gscite.tgz
mkdir gscite
cp scite/license.txt gscite
cp scite/bin/SciTE gscite
strip gscite/SciTE
cp scite/gtk/SciTEGTK.properties gscite/SciTEGlobal.properties
cp scite/doc/*.html gscite
cp scite/doc/*.png gscite
cp scite/doc/*.jpg gscite
tar --create gscite/* | gzip -c >gscite.tgz
rm -f gscite/*
rmdir gscite
